        <FromCondition TableName="T_UR_SH_ENTRY" />
        <!-- 代納JOINも明細のJOINクエリに入れる -->
        <JoinCondition TableName="T_UR_SH_DETAIL">
            INNER JOIN T_UR_SH_DETAIL ON T_UR_SH_DETAIL.SYSTEM_ID = T_UR_SH_ENTRY.SYSTEM_ID AND T_UR_SH_DETAIL.SEQ = T_UR_SH_ENTRY.SEQ
            INNER JOIN (
            SELECT
            T1.SYSTEM_ID AS UKEIRE_SYSTEM_ID, T1.SEQ AS UKEIRE_SEQ, T1.DETAIL_SYSTEM_ID AS UKEIRE_DETAIL_SYSTEM_ID,
            T2.SYSTEM_ID AS SHUKKA_SYSTEM_ID, T2.SEQ AS SHUKKA_SEQ, T2.DETAIL_SYSTEM_ID AS SHUKKA_DETAIL_SYSTEM_ID
            FROM (
            SELECT H.UR_SH_NUMBER, D.SYSTEM_ID, D.SEQ, D.DETAIL_SYSTEM_ID, D.ROW_NO, H.DELETE_FLG
            FROM T_UR_SH_ENTRY H INNER JOIN T_UR_SH_DETAIL D ON D.SYSTEM_ID = H.SYSTEM_ID AND D.SEQ = H.SEQ AND D.DENPYOU_KBN_CD = 2
            WHERE H.DELETE_FLG = 0 AND H.DAINOU_FLG = 1
            ) T1
            INNER JOIN (
            SELECT H.UR_SH_NUMBER, D.SYSTEM_ID, D.SEQ, D.DETAIL_SYSTEM_ID, D.ROW_NO, H.DELETE_FLG
            FROM T_UR_SH_ENTRY H INNER JOIN T_UR_SH_DETAIL D ON D.SYSTEM_ID = H.SYSTEM_ID AND D.SEQ = H.SEQ AND D.DENPYOU_KBN_CD = 1
            WHERE H.DELETE_FLG = 0 AND H.DAINOU_FLG = 1
            ) T2 ON
            T2.UR_SH_NUMBER = T1.UR_SH_NUMBER AND T2.ROW_NO = T1.ROW_NO
            ) T_DAINOU_DETAIL_SHUKKA ON
            T_UR_SH_DETAIL.SYSTEM_ID = T_DAINOU_DETAIL_SHUKKA.SHUKKA_SYSTEM_ID AND
            T_UR_SH_DETAIL.SEQ = T_DAINOU_DETAIL_SHUKKA.SHUKKA_SEQ AND
            T_UR_SH_DETAIL.DETAIL_SYSTEM_ID = T_DAINOU_DETAIL_SHUKKA.SHUKKA_DETAIL_SYSTEM_ID
        </JoinCondition>
        <JoinCondition TableName="T_SEIKYUU_DETAIL1">LEFT JOIN T_SEIKYUU_DETAIL T_SEIKYUU_DETAIL1 ON T_UR_SH_DETAIL.SYSTEM_ID = T_SEIKYUU_DETAIL1.DENPYOU_SYSTEM_ID AND T_UR_SH_DETAIL.SEQ = T_SEIKYUU_DETAIL1.DENPYOU_SEQ AND T_SEIKYUU_DETAIL1.DENPYOU_SHURUI_CD = 3 AND T_SEIKYUU_DETAIL1.DELETE_FLG = 0</JoinCondition>
        <JoinCondition TableName="M_KYOTEN1">LEFT JOIN M_KYOTEN M_KYOTEN1 ON T_UR_SH_ENTRY.KYOTEN_CD = M_KYOTEN1.KYOTEN_CD</JoinCondition>
        <JoinCondition TableName="M_TORIHIKI_KBN1">LEFT JOIN M_TORIHIKI_KBN M_TORIHIKI_KBN1 ON T_UR_SH_ENTRY.URIAGE_TORIHIKI_KBN_CD = M_TORIHIKI_KBN1.TORIHIKI_KBN_CD</JoinCondition>
        <JoinCondition TableName="M_TORIHIKI_KBN2">LEFT JOIN M_TORIHIKI_KBN M_TORIHIKI_KBN2 ON T_UR_SH_ENTRY.SHIHARAI_TORIHIKI_KBN_CD = M_TORIHIKI_KBN2.TORIHIKI_KBN_CD</JoinCondition>
        <JoinCondition TableName="M_DENPYOU_KBN1">LEFT JOIN M_DENPYOU_KBN M_DENPYOU_KBN1 ON T_UR_SH_DETAIL.DENPYOU_KBN_CD = M_DENPYOU_KBN1.DENPYOU_KBN_CD</JoinCondition>
        <JoinCondition TableName="M_UNIT1">LEFT JOIN M_UNIT M_UNIT1 ON T_UR_SH_DETAIL.UNIT_CD = M_UNIT1.UNIT_CD</JoinCondition>
        <JoinCondition TableName="T_UR_SH_DETAIL_SUM">
            INNER JOIN
            (SELECT
            SYSTEM_ID,
            SEQ,
            <!--SUM(STACK_JYUURYOU) STACK_JYUURYOU_SUM,-->
            SUM(NET_JYUURYOU) NET_JYUURYOU_SUM
            FROM
            T_UR_SH_DETAIL
            GROUP BY
