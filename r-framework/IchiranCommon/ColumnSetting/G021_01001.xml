    <JoinCondition TableName="M_MANIFEST_TEHAI1">LEFT JOIN M_MANIFEST_TEHAI M_MANIFEST_TEHAI1 ON T_UKETSUKE_SS_ENTRY.MANIFEST_TEHAI_CD = M_MANIFEST_TEHAI1.MANIFEST_TEHAI_CD</JoinCondition>
    <JoinCondition TableName="M_CONTENA_JOUKYOU1">LEFT JOIN M_CONTENA_JOUKYOU M_CONTENA_JOUKYOU1 ON T_UKETSUKE_SS_ENTRY.CONTENA_SOUSA_CD = M_CONTENA_JOUKYOU1.CONTENA_JOUKYOU_CD</JoinCondition>
    <JoinCondition TableName="M_COURSE_NAME1">LEFT JOIN M_COURSE_NAME M_COURSE_NAME1 ON T_UKETSUKE_SS_ENTRY.COURSE_NAME_CD = M_COURSE_NAME1.COURSE_NAME_CD</JoinCondition>
    <JoinCondition TableName="M_DENPYOU_KBN1">LEFT JOIN M_DENPYOU_KBN M_DENPYOU_KBN1 ON T_UKETSUKE_SS_DETAIL.DENPYOU_KBN_CD = M_DENPYOU_KBN1.DENPYOU_KBN_CD</JoinCondition>
    <JoinCondition TableName="M_UNIT1">LEFT JOIN M_UNIT M_UNIT1 ON T_UKETSUKE_SS_DETAIL.UNIT_CD = M_UNIT1.UNIT_CD</JoinCondition>
    <JoinCondition TableName="URIAGE">
      LEFT JOIN
      (
      SELECT
      E.SYSTEM_ID,
      E.SEQ,
      SUM(D.KINGAKU) AS KINGAKU_TOTAL
      FROM
      T_UKETSUKE_SS_ENTRY E,
      T_UKETSUKE_SS_DETAIL D
      WHERE
      E.DELETE_FLG = 0 AND
      E.SYSTEM_ID = D.SYSTEM_ID AND
      E.SEQ = D.SEQ AND
      D.DENPYOU_KBN_CD = 1
      GROUP BY
      E.SYSTEM_ID,
      E.SEQ
      ) URIAGE ON
      T_UKETSUKE_SS_ENTRY.SYSTEM_ID = URIAGE.SYSTEM_ID AND
      T_UKETSUKE_SS_ENTRY.SEQ = URIAGE.SEQ
    </JoinCondition>
    <JoinCondition TableName="SHIHARAI">
      LEFT JOIN
      (
      SELECT
      E.SYSTEM_ID,
      E.SEQ,
      SUM(D.KINGAKU) AS KINGAKU_TOTAL
      FROM
      T_UKETSUKE_SS_ENTRY E,
      T_UKETSUKE_SS_DETAIL D
      WHERE
      E.DELETE_FLG = 0 AND
      E.SYSTEM_ID = D.SYSTEM_ID AND
      E.SEQ = D.SEQ AND
      D.DENPYOU_KBN_CD = 2
      GROUP BY
      E.SYSTEM_ID,
      E.SEQ
      ) SHIHARAI ON
      T_UKETSUKE_SS_ENTRY.SYSTEM_ID = SHIHARAI.SYSTEM_ID AND
      T_UKETSUKE_SS_ENTRY.SEQ = SHIHARAI.SEQ
    </JoinCondition>
    <JoinCondition TableName="T_LOGI_DELIVERY_DETAIL">LEFT JOIN T_LOGI_DELIVERY_DETAIL ON T_UKETSUKE_SS_ENTRY.SYSTEM_ID = T_LOGI_DELIVERY_DETAIL.REF_SYSTEM_ID AND T_LOGI_DELIVERY_DETAIL.DENPYOU_ATTR = 1</JoinCondition>
    <JoinCondition TableName="T_GENBAMEMO_ENTRY">LEFT JOIN T_GENBAMEMO_ENTRY ON T_UKETSUKE_SS_ENTRY.UKETSUKE_NUMBER = T_GENBAMEMO_ENTRY.HASSEIMOTO_NUMBER</JoinCondition>
    <JoinCondition TableName="T_SMS">LEFT JOIN T_SMS ON T_UKETSUKE_SS_ENTRY.UKETSUKE_NUMBER = T_SMS.DENPYOU_NUMBER</JoinCondition>
  </OutputColumnSelect>
