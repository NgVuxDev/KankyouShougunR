      <Column ID="7" DispNumber="5" DispName="作業開始時間" Needs="0" TableName="" Name="RIGHT('00' + CONVERT(VARCHAR, T_TEIKI_HAISHA_ENTRY.SAGYOU_BEGIN_HOUR), 2) + ':' + RIGHT('00' + CONVERT(VARCHAR, T_TEIKI_HAISHA_ENTRY.SAGYOU_BEGIN_MINUTE), 2)" Format="" />
