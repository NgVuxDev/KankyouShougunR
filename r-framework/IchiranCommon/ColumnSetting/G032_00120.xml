      <Column ID="15" DispNumber="14" DispName="運転者CD" Needs="0" TableName="T_TEIKI_HAISHA_ENTRY" Name="UNTENSHA_CD" Format="" />
      <Column ID="16" DispNumber="15" DispName="運転者名" Needs="0" TableName="M_SHAIN1" Name="SHAIN_NAME_RYAKU" Format="" />
      <Column ID="17" DispNumber="16" DispName="補助員CD" Needs="0" TableName="T_TEIKI_HAISHA_ENTRY" Name="HOJOIN_CD" Format="" />
