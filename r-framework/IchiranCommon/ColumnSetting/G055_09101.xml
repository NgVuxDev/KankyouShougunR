      <Column ID="28" DispNumber="31" DispName="品名別税区分" Needs="0" TableName="" Name="CASE T_UKEIRE_DETAIL.HINMEI_ZEI_KBN_CD WHEN 1 THEN '外税' WHEN 2 THEN '内税' WHEN 3 THEN '非課税' ELSE '' END" Format="" />
