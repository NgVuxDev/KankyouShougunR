    <JoinCondition TableName="M_KYOTEN1">LEFT JOIN M_KYOTEN M_KYOTEN1 ON T_UKEIRE_ENTRY.KYOTEN_CD = M_KYOTEN1.KYOTEN_CD</JoinCondition>
    <JoinCondition TableName="M_TORIHIKISAKI1">LEFT JOIN M_TORIHIKISAKI M_TORIHIKISAKI1 ON T_UKEIRE_ENTRY.TORIHIKISAKI_CD = M_TORIHIKISAKI1.TORIHIKISAKI_CD</JoinCondition>
    <JoinCondition TableName="M_GYOUSHA1">LEFT JOIN M_GYOUSHA M_GYOUSHA1 ON T_UKEIRE_ENTRY.GYOUSHA_CD = M_GYOUSHA1.GYOUSHA_CD</JoinCondition>
    <JoinCondition TableName="M_GENBA1">LEFT JOIN M_GENBA M_GENBA1 ON T_UKEIRE_ENTRY.GYOUSHA_CD = M_GENBA1.GYOUSHA_CD AND T_UKEIRE_ENTRY.GENBA_CD = M_GENBA1.GENBA_CD</JoinCondition>
    <JoinCondition TableName="M_KEITAI_KBN1">LEFT JOIN M_KEITAI_KBN M_KEITAI_KBN1 ON T_UKEIRE_ENTRY.KEITAI_KBN_CD = M_KEITAI_KBN1.KEITAI_KBN_CD</JoinCondition>
    <JoinCondition TableName="M_CONTENA_JOUKYOU1">LEFT JOIN M_CONTENA_JOUKYOU M_CONTENA_JOUKYOU1 ON T_UKEIRE_ENTRY.CONTENA_SOUSA_CD = M_CONTENA_JOUKYOU1.CONTENA_JOUKYOU_CD</JoinCondition>
    <JoinCondition TableName="M_MANIFEST_SHURUI1">LEFT JOIN M_MANIFEST_SHURUI M_MANIFEST_SHURUI1 ON T_UKEIRE_ENTRY.MANIFEST_SHURUI_CD = M_MANIFEST_SHURUI1.MANIFEST_SHURUI_CD</JoinCondition>
    <JoinCondition TableName="M_MANIFEST_TEHAI1">LEFT JOIN M_MANIFEST_TEHAI M_MANIFEST_TEHAI1 ON T_UKEIRE_ENTRY.MANIFEST_TEHAI_CD = M_MANIFEST_TEHAI1.MANIFEST_TEHAI_CD</JoinCondition>
    <JoinCondition TableName="M_TORIHIKI_KBN1">LEFT JOIN M_TORIHIKI_KBN M_TORIHIKI_KBN1 ON T_UKEIRE_ENTRY.URIAGE_TORIHIKI_KBN_CD = M_TORIHIKI_KBN1.TORIHIKI_KBN_CD</JoinCondition>
    <JoinCondition TableName="M_TORIHIKI_KBN2">LEFT JOIN M_TORIHIKI_KBN M_TORIHIKI_KBN2 ON T_UKEIRE_ENTRY.SHIHARAI_TORIHIKI_KBN_CD = M_TORIHIKI_KBN2.TORIHIKI_KBN_CD</JoinCondition>
    <JoinCondition TableName="M_DENPYOU_KBN1">LEFT JOIN M_DENPYOU_KBN M_DENPYOU_KBN1 ON T_UKEIRE_DETAIL.DENPYOU_KBN_CD = M_DENPYOU_KBN1.DENPYOU_KBN_CD</JoinCondition>
    <JoinCondition TableName="M_YOUKI1">LEFT JOIN M_YOUKI M_YOUKI1 ON T_UKEIRE_DETAIL.YOUKI_CD = M_YOUKI1.YOUKI_CD</JoinCondition>
    <JoinCondition TableName="M_UNIT1">LEFT JOIN M_UNIT M_UNIT1 ON T_UKEIRE_DETAIL.UNIT_CD = M_UNIT1.UNIT_CD</JoinCondition>
    <JoinCondition TableName="M_UNIT2">LEFT JOIN M_UNIT M_UNIT2 ON T_UKEIRE_DETAIL.NISUGATA_UNIT_CD = M_UNIT2.UNIT_CD</JoinCondition>
    <JoinCondition TableName="T_MANIFEST_ENTRY">LEFT JOIN (SELECT * FROM T_MANIFEST_ENTRY WHERE DELETE_FLG = 0) T_MANIFEST_ENTRY ON T_UKEIRE_DETAIL.SYSTEM_ID = T_MANIFEST_ENTRY.RENKEI_SYSTEM_ID AND T_UKEIRE_DETAIL.DETAIL_SYSTEM_ID = T_MANIFEST_ENTRY.RENKEI_MEISAI_SYSTEM_ID AND T_MANIFEST_ENTRY.RENKEI_DENSHU_KBN_CD = 1</JoinCondition>
    <JoinCondition TableName="T_ZAIKO_HINMEI_HURIWAKE">
      LEFT OUTER JOIN
      (SELECT
      SYSTEM_ID, SEQ, DETAIL_SYSTEM_ID, ZAIKO_HINMEI_COUNT,
      CASE WHEN ZAIKO_HINMEI_COUNT = 1 THEN ZAIKO_HINMEI_CD ELSE '' END ZAIKO_HINMEI_CD,
      CASE WHEN ZAIKO_HINMEI_COUNT = 1 THEN ZAIKO_HINMEI_NAME ELSE '複数在庫品名' END ZAIKO_HINMEI_NAME
      FROM
      (SELECT
      SYSTEM_ID, SEQ, DETAIL_SYSTEM_ID,
      MIN(ZAIKO_HINMEI_CD) ZAIKO_HINMEI_CD,
      MIN(ZAIKO_HINMEI_NAME) ZAIKO_HINMEI_NAME,
      COUNT(*) ZAIKO_HINMEI_COUNT
      FROM
      T_ZAIKO_HINMEI_HURIWAKE
      WHERE
      DENSHU_KBN_CD = '1'
      GROUP BY
      SYSTEM_ID, SEQ, DETAIL_SYSTEM_ID
      ) T
      ) T_ZAIKO_HINMEI_HURIWAKE
      ON
      T_UKEIRE_DETAIL.SYSTEM_ID = T_ZAIKO_HINMEI_HURIWAKE.SYSTEM_ID AND
      T_UKEIRE_DETAIL.SEQ = T_ZAIKO_HINMEI_HURIWAKE.SEQ AND
      T_UKEIRE_DETAIL.DETAIL_SYSTEM_ID = T_ZAIKO_HINMEI_HURIWAKE.DETAIL_SYSTEM_ID
    </JoinCondition>
    <JoinCondition TableName="M_SHURUI">
      LEFT JOIN
      (
      SELECT MH.HINMEI_CD, MH.SHURUI_CD, MS.SHURUI_NAME_RYAKU FROM
      M_HINMEI MH
      INNER JOIN M_SHURUI MS ON MS.SHURUI_CD = MH.SHURUI_CD
      ) M_SHURUI
      ON M_SHURUI.HINMEI_CD = T_UKEIRE_DETAIL.HINMEI_CD
    </JoinCondition>
    <JoinCondition TableName="M_BUNRUI">
      LEFT JOIN
      (
      SELECT MH.HINMEI_CD, MH.BUNRUI_CD, MB.BUNRUI_NAME_RYAKU FROM
      M_HINMEI MH
      INNER JOIN M_BUNRUI MB ON MB.BUNRUI_CD = MH.BUNRUI_CD
      ) M_BUNRUI
      ON M_BUNRUI.HINMEI_CD = T_UKEIRE_DETAIL.HINMEI_CD
    </JoinCondition>
    <JoinCondition TableName="HM2">LEFT JOIN M_HINMEI HM2 ON T_UKEIRE_JISSEKI_DETAIL.HINMEI_CD = HM2.HINMEI_CD</JoinCondition>
